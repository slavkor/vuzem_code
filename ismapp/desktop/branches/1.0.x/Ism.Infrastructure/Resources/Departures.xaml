                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource Timeline_TimelineInstantItem_Fill_Pressed}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
